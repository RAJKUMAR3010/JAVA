import java.util.*;

class Main{
    public static void main(String args[]){
        String str = "Vignesh";
        int n = str.length();
        char[] ch = new char[n];
        int j=0;
        for(int i=n-1;i>=0;i--){
            ch[j] = str.charAt(i);
            j++;
        }
        for(int i=0;i<n;i++){
            System.out.print(ch[i]);
        }
    }
}
